<div class="container">
  <h2>My Bookings</h2>
  <table class="table table-striped" style="margin-bottom: 238px;">
    <thead>
    
      <tr>
        <th>Travel</th>
        <th>Bus Type</th>
        <th>From</th>
        <th>To</th>
        <th>Number of Seats</th>
        <th>Total Amount</th>
        <th>Cancel Ticket</th>
      </tr>
    </thead>
    <tbody>
    <!-- JSTL DbConnection -->
<sql:setDataSource var="db" driver="com.mysql.jdbc.Driver" url="jdbc:mysql://localhost:3306/db_travels" user="root"  password="password"/>  
<sql:query dataSource="${db}" var="rs"> SELECT * FROM `ticket` WHERE uemail='<%=user.getAttribute("uemail")%>' </sql:query>

    <c:forEach var="result" items="${rs.rows}">
      <tr>
        <td>${result.travel}</td>
        <td>${result.type}</td>
        <td>${result.from}</td>
        <td>${result.to}</td>
        <td>${result.seats}</td>
        <td>${result.amount}</td>
        <td><a href="CancelTicket?id=${result.ticketid}" onclick="return confirm('Do you really want to cancel?')">Cancel</a></td>
      </tr>
     </c:forEach>
      
    </tbody>
  </table>
</div>







